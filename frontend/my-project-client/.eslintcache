ndleLogin=()=>{\n      if(localStorage.getItem('auth_key')){\n        this.setState({isLoggedIn: true})\n        this.createCart()\n      }\n    }\n\n    createCart=()=>{\n      const newCart={\n          total:0.0\n      }\n\n      fetch(cartUrl,{\n          method: 'POST',\n          headers:{\n              'Content-Type': 'application/json',\n              'Auth-Key': localStorage.getItem('auth_key')\n          },\n          body: JSON.stringify(newCart)\n          })\n          .then(res=>res.json())\n          .then(cart=> {\n          //    console.log(cart)\n          localStorage.setItem('cart_id', cart.id)\n      })\n    } \n\n    render(){\n      \n      return (\n        <div className=\"App\">\n          <Router> \n            <NavBar handleLogin={this.handleLogin} isLoggedIn={this.state.isLoggedIn} />\n            <Route exact path =\"/\" component= {LandingPage} />\n            <Route exact path =\"/menu\" component={Menu} />\n            <Route exact path =\"/contact\" component= {Contact} />\n            <Route exact path =\"/order\" component= {()=>{\n              if(localStorage.getItem('auth_key')){\n                return <OrderContainer />\n              }else{\n                return <Redirect to=\"/login\"/>\n              }}} /> \n            <Route exact path =\"/cart\" component= {()=>{\n              if(localStorage.getItem('auth_key')){\n                return <Cart />\n              }else{\n                return <Redirect to=\"/login\"/>\n              }}} /> \n            <Route exact path =\"/signup\" component= {()=><SignUp handleLogin={this.handleLogin}/>}/>\n            <Route exact path =\"/login\" component= {()=>{\n              return <Login handleLogin={this.handleLogin}/>\n            }}/>\n            <Route exact path =\"/logout\" component = {()=>{\n              localStorage.clear()\n              this.setState({isLoggedIn: false})\n              return <Redirect to=\"/\"/>\n            }}/>\n            <FooterBar> </FooterBar>\n          </Router>\n        </div>\n      );\n\n    }\n\n}\n\n\nexport default App;\n",["99","100"],"/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/LandingPage.js",["101"],"import React from 'react'\nimport {Carousel, Container, Row, Col } from 'react-bootstrap'\n\n\n\nclass LandingPage extends React.Component {\n\n    render(){\n\n        const row = {\n            backgroundColor: '#FBFBFB'\n        }\n        const right = {\n            fontSize: 50,\n            color: \"#FBFBFB\",\n            fontFamily: \"Roboto\"\n        } \n        const middle = {\n            fontSize: 20,\n            fontFamily: \"Roboto\",\n            marginRight: \"50\"\n        }\n    \n    return (\n        <Container fluid> \n            <Carousel>\n                <Carousel.Item interval={5000}>\n                    <img\n                    className=\"d-block w-100\"\n                    src=\"https://live.staticflickr.com/65535/50808152531_478185749a_o.png\"\n                    alt=\"First slide\"\n                    />\n                </Carousel.Item>\n                <Carousel.Item interval={5000}>\n                    <img\n                    className=\"d-block w-100\"\n                    src=\"https://live.staticflickr.com/65535/50807410613_55070765c2_o.png\"\n                    alt=\"Third slide\"\n                    />\n                   \n                </Carousel.Item>\n                <Carousel.Item interval={5000}>\n                    <img\n                    className=\"d-block w-100\"\n                    src=\"https://live.staticflickr.com/65535/50751730128_8f06f57bd5_o.png\"\n                    alt=\"Second slide\"\n                    />\n                </Carousel.Item>\n            </Carousel> \n            <Row style={row} > \n                <Col style={right}> . </Col>\n                <Col> hello </Col>\n            </Row>\n        </Container>\n\n\n    )}\n}\n\nexport default LandingPage\n\n\n","/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/Contact.js",["102"],"import React from 'react'\nimport { Container, Row, Col } from 'react-bootstrap'\n\n\nfunction Contact() {\n\n    const style = {\n        fontFamily: \"Russo One\",\n        fontSize: 35,\n        backgroundColor: '#F1F1E2',\n        justifyContent: 'center'\n    }\n\n    const sub = {\n        textAlign: \"center\",\n        fontFamily: \"PT Sans\"\n    }\n\n    return (\n        <div>\n            <Container> \n                <Row style={style}> L O C A T I O N </Row> <br></br>\n                <Row style={sub}> Just a short walk from NYU, Silver Spurs on Houston St. & LaGuardia Pl. in New York is the perfect place to enjoy a colossal burger, brunch on the weekends, or drinks with your friends. </Row> <br></br>\n                <Row style={style}> C O N T A C T </Row> <br></br>\n                <Row style={sub}> Text text text</Row> <br></br>\n                <Row style={style}> A B O U T </Row><br></br>\n                <Row style={sub}> Tired of the same old diners in Manhattan? Look no further than Silver Spurs for a truly delicious and unique diner experience. Our extensive menu is sure to please anyone. From our appeteasers to our famous colossal burgers, sandwiches, wraps, paninis and desserts, Silver Spurs is good eating at very low prices.\n                    Visit this neighborhood favorite for breakfast, lunch, or dinner, or order in on a rainy or lazy day! We offer brunch on Saturday and Sunday and have a full bar. At Silver Spurs, everything is made fresh to order and all of our meat is cut fresh at our butcher shop.\n                    Silver Spurs is also available to cater your next party or corporate event. Please view our catering menu and place an order online.\n                    Since opening in 1979, Silver Spurs has set out to be a unique diner in Manhattan that offers fresh food, good drinks and delicious eats. Near NYU, students, professionals, police officers and local neighborhood people come to Silver Spurs for our burgers, paninis, salads, sandwiches and brunch.\n                    Some of the local favorite dishes include: Salada Lailai, The Silver Spurs Burger, Mickey Maffosa Sandwich, Fryin’ Friers, El Cubano Ruben Panini, and the Ms. Annie Oakley Sandwich. <br></br><br></br>\n                    Created by Saief, Jake, & Gabbi </Row>\n            </Container>\n        </div>\n    )\n}\n\nexport default Contact;","/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/SignUp.js",[],"/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/OrderContainer.js",["103","104","105","106"],"/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/OrderBtn.js",["107"],"/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/NavBar.js",["108","109","110","111"],"/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/MenuCol1.js",[],"/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/MenuItem.js",[],"/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/CartChild.js",[],"/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/map/Map.js",[],"/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/CartSideBtn.js",[],"/Users/saiefsayed/Flatiron/code/Restaurant/frontend/my-project-client/src/components/CartContainer.js",[],{"ruleId":"112","severity":1,"message":"113","line":4,"column":8,"nodeType":"114","messageId":"115","endLine":4,"endColumn":21},{"ruleId":"116","severity":1,"message":"117","line":61,"column":80,"nodeType":"118","messageId":"119","endLine":61,"endColumn":82},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","replacedBy":"123"},{"ruleId":"120","replacedBy":"124"},{"ruleId":"122","replacedBy":"125"},{"ruleId":"